{
  "name": "book-nook",
  "version": "1.0.0",
  "description": "Book searching & saving app",
  "main": "server.js",
  "proxy": "http://localhost:3000",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js && npm run client",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonesElliott/book-nook.git"
  },
  "author": "JonesElliott",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JonesElliott/book-nook/issues"
  },
  "homepage": "https://github.com/JonesElliott/book-nook#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongodb": "^3.6.3",
    "mongojs": "^3.1.0",
    "mongoose": "^5.10.17",
    "morgan": "^1.10.0",
    "serve": "^11.3.2"
  }
}
